<regex>
        <name>token</name>
        <expres>token:"(.*?)"</expres>
        <page>$doregex[decrypted]</page>
</regex>
<regex>
        <name>authurl</name>
        <expres>url:'(.*?)'</expres>
        <page>$doregex[decrypted]</page>
</regex>
<regex>
        <name>decrypted</name>
        <expres>$pyFunction:get_unwise("$doregex[encrypted]")$doregex[savecookie]</expres>
        <page></page>
</regex>
<regex>
        <name>encrypted</name>
        <expres>eval\(function\(w,i,s,e\).*}\((.*?)\)</expres>
        <page>http://www.caston.tv/player.php?id=12</page>
        <referer>http://www.caston.tv</referer>
        <agent>iPhone</agent>
        <cookieJar></cookieJar>
</regex>
<regex>
        <name>savecookie</name>
        <expres></expres>
        <page></page>
        <cookieJar>save[siteName.lwp]</cookieJar>
</regex>
