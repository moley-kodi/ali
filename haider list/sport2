<item>
<title>Caston Regex</title>
<link>$doregex[finalurl]</link>
<regex>
        <name>finalurl</name>
        <expres>(http.*)</expres>
        <page>$doregex[url]$doregex[auth]</page>
        <cookieJar></cookieJar>
        <agent>iPhone</agent>
</regex>
<regex>
        <name>url</name>
        <expres>file:"(.*?)"</expres>
        <page>$doregex[decrypted]</page>
</regex>
<regex>
        <name>auth</name>
        <expres>\["(.*?)"</expres>
        <page>http://www.caston.tv/$doregex[authurl]</page>
        <referer>http://www.caston.tv</referer>
        <rawpost>token=$doregex[token]&amp;is_ajax=1</rawpost>
        <x-req>XMLHttpRequest</x-req>
        <agent>iPhone</agent>
        <cookieJar>open[siteName.lwp]</cookieJar>
</regex>
<regex>
        <name>token</name>
        <expres>token:"(.*?)"</expres>
        <page>$doregex[decrypted]</page>
</regex>
<regex>
        <name>authurl</name>
        <expres>url:'(.*?)'</expres>
        <page>$doregex[decrypted]</page>
</regex>
<regex>
        <name>decrypted</name>
        <expres>$pyFunction:get_unwise("$doregex[encrypted]")$doregex[savecookie]</expres>
        <page></page>
</regex>
<regex>
        <name>encrypted</name>
        <expres>eval\(function\(w,i,s,e\).*}\((.*?)\)</expres>
        <page>http://www.caston.tv/player.php?id=27</page>
        <referer>http://www.caston.tv</referer>
        <agent>iPhone</agent>
        <cookieJar></cookieJar>
</regex>
<regex>
        <name>savecookie</name>
        <expres></expres>
        <page></page>
        <cookieJar>save[siteName.lwp]</cookieJar>
</regex>
